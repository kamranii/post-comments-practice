@model List<Models.Comment>
@{
}

@foreach (var comment in Model)
{
    <div>
        <h6>Name: @comment.Name</h6>
        <p>@comment.Body</p>
        <p class="text-danger">Email: @comment.Email</p>
    </div>
}
